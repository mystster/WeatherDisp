{
  "timezone": "Asia/Tokyo",
  "masterIssue": true,
  "minor": {
    "labels": ["minor"]
  },
  "major": {
    "labels": ["major"]
  },
  "js": {
    "packageRules": [
      {
        "packagePatterns": [
          "*"
        ],
        "minor": {
          "groupName": "all non-major dependencies",
          "groupSlug": "all-minor-patch"
        }
      },
      {
        "groupName": "ESLint and Prettier",
        "packageNames": [
          "eslint",
          "prettier"
        ],
        "packagePatterns": [
          "^eslint-config-",
          "^eslint-plugin-"
        ]
      },
      {
        "groupName": "Vue CLI",
        "packagePatterns": [
          "^@vue/"
        ]
      }
    ]
  },
  "extends": [
    "config:base",
    "helpers:disableTypesNodeMajor",
    "schedule:weekdays",
    ":label(dependencies)",
    ":assignee(mystster)",
    ":prConcurrentLimit10"
  ]
}
